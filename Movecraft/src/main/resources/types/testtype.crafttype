movecraft-bridges:
  allow_bridge_signs: true
movecraft-combat:
  #Tracers
  movement_tracer_blocks:
    - "#tte:engines"
  movement_tracer_chance: 1.0
  max_movement_tracers: -1
  #Directors
  allow_aa_director_sign: true
  max_aa_director_angle: 45.0
  allow_arrow_director_sign: true
  max_arrow_director_angle: 45.0
  allow_cannon_director_sign: true
  max_cannon_director_angle: 45.0
movecraft-tte-additions:
  #BlastPowerOverride
  tnt_blast_power_override: 6.0
  #Command Restrictor
  enforce_command_restrictions: true
  #Sounds
  #Cruise rework
  engines_define_cruise_direction: true
  ignore_during_subcraft_detection:
    - LaunchBomb
    - LaunchTorpedo
    - Turret
    - TurretS
    - TurretM
    - TurretC
    - TurretDD
    - Lightairship
    - Airship
    - BigAirship
    - MegaAirship
    - Ultrairship
    - GigAirship
  #Warheads