forbiddenBlocks:
    - piston_head

canCruise: true
rotateAtMidpoint: true
blockRemoteRotate: true
cruiseSkipBlocks: 12
cruiseSpeed: 0.17
canFly: false
minHeightLimit: 1
maxHeightLimit: 110
maxHeightAboveGround: 50
vertCruiseSkipBlocks: 4
detectionMultiplier: 10.0
underwaterDetectionMultiplier: 1.0
speed: 0.17
collisionExplosion: 0.0
fuelBurnRate: 4.0
tryNudge: false
sinkPercent: 99.0
overallSinkPercent: 60.0
halfSpeedUnderwater: true
moveblocks:
    redstone_block: # redstone represents engines
        - 1.0
        - 100.0
    iron_block: # iron represents ballast tanks
        - 10.0
        - 100.0
flyblocks:
    "#wool": #wool represents lift
        - 50.0
        - 100.0
    "#chests": # limit chests to make big airships important for merchant missions
        - 0.0
        - 1.0
    [dispenser,hopper,dropper]: # dispensers and hoppers can be used for inventory too, so limit them as well
        - 0.0
        - 2.0
    furnace: # Furnaces for cargo? really guys?
        - 0.0
        - N50
    "#anvil": # anvils are OP armor, so limit it
        - 0.0
        - 1.0
    obsidian: # so is obsidian
        - 0.0
        - 1.0
    water: # so is water
        - 0.0
        - 1.0
passthroughBlocks:
    - kelp_plant
    - seagrass
    - kelp
    - tall_seagrass
    - bubble_column
